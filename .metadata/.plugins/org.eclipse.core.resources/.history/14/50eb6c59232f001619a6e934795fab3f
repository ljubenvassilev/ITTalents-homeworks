package hw5;

public class Zad11do14 {

	public static void main(String[] args) {
		
		int[] array =createArray(5);
		printArray(array);
		
	}
	static void printArray(int[] arr){
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i]+" ");
		}
		System.out.println();
	}
	static int[] createArray(int n){
		int[] arr= new int[n];
		for (int i = 0; i < arr.length; i++) {
			arr[i]=i+1;
		}
		return arr;
	}
	
}
