package hw2;

import java.util.Scanner;

public class Zad21 {
	
	public static void main (String[] args){
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter number:");
		
		int n = sc.nextInt();
		int karta=(n+1)%13;
		int boq=(n+1)/13;
		for (int i=karta;i<=13;i++){
			for (int j=boq;j<=4;j++){
				System.out.println(i);
				System.out.println(j);
			}
		}
		
	}
}
