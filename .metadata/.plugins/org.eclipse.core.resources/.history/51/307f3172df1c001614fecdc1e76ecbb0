package hw2;

import java.util.Scanner;

public class Zad22 {
	
	public static void main (String[] args){
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter number:");
		
		int n = sc.nextInt();
		
		int k = 1;
		
		while(k<=10){
			if(n%2==0||n%3==0||n%5==0){
				System.out.print(k+":"+n+"; ");
				k++;
			}
			n++;
						
		}

	}

}
