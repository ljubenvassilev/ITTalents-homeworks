package notes;

class Page {

	private String caption;
	private String content;
	
	Page() {}
	
	Page(String caption){
		super();
		this.caption=caption;
	}
	
	void addText(String addition){
		content+=addition;
	}
	
	void deleteText(){
		content="";
	}
	
	String view(){
		return caption+"\n"+content;
	}
}
