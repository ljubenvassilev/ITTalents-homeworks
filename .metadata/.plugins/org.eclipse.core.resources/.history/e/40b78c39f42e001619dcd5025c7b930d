package hw5;
import java.util.Scanner;
public class Zad4 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in).useDelimiter("\\s*,\\s*");
		String name1;
		String name2;
		System.out.println("Input two names:");
		name1 = sc.next();
		name2 = sc.next();

		System.out.println(name1);
		
	}
	

}
