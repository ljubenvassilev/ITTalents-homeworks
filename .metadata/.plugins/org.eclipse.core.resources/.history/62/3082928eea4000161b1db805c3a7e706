package HW7;


public class Computer {

	int year;
	double price;
	boolean isNotebook;
	int hardDiskMemory;
	int freeMemory;
	String operationSystem;
	
	public void changeOperationSystem(String s){
		operationSystem=s;
	}
	
	public void useMemory(int use){
		if (use>freeMemory){
			System.out.println("Not enough free memory!");
			return;
		}
		freeMemory-=use;
	}
}
